<launch>
    <node name="cmd_vel_twist_to_float" pkg="buildon_main" type="cmd_vel_twist_to_float.py"/>
    <node name="rqt_robot_steering" pkg="rqt_robot_steering" type="rqt_robot_steering"/>
    <node name="vel_plot"
        pkg="rqt_plot"
        type="rqt_plot"
        args="/right_wheel_vel/data /left_wheel_vel/data /cmd_vel_linear/data /cmd_vel" />
    <arg name="rvizconfig" default="$(find buildon_main)/rviz/05_ydlidar_test.rviz" />
    <node name="rviz" pkg="rviz" type="rviz" args="-d $(arg rvizconfig)" required="true" />
</launch>
